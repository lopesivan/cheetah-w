// Zona de pesquisa direta.
zone "42algoritmos.local" {
  type master;
  file "/etc/bind/42algoritmos/db.42algoritmos.local";
};

// Zona de pesquisa reversa.
zone "2.168.192.in-addr-arpa" {
  type master;
  file "/etc/bind/42algoritmos/db.192";
};


